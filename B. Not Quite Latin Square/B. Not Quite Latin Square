#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t-->0){
        //code here
 
        
        vector<vector<char> > arr(3,vector<char>(3));
        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
                cin>>arr[i][j];
            }
        }
 
        int a=0,b=0,c=0;
           for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
            if(arr[i][j]=='A') a++;
            else if(arr[i][j]=='B') b++;
            else c++;
            }
        }
 
         if(a==2) cout<<'A'<<endl;
        else if(b==2) cout<<'B'<<endl;
        else cout<<'C'<<endl;
    
 
 
 
 
 
 
 
    }
}
